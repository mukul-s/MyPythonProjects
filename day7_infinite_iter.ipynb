    "class even_infinite:\n",
    "    \"\"\"This class return infinite number to iterate over if\n",
    "    sentinel is not given\"\"\"\n",
    "    def __iter__(self):\n",
    "        self.num = 2\n",
    "        return self\n",
    "    def __next__(self):\n",
    "        self.num += 2\n",
    "        return self.num"
